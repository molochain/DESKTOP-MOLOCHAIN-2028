# Laravel CMS Environment Configuration
# Copy this file to .env and update values

# Application
APP_KEY=base64:your-32-character-key-here-in-base64
APP_ENV=production
APP_DEBUG=false
APP_URL=https://cms.molochain.com

# Database
CMS_DB_NAME=cmsdb
CMS_DB_USER=cmsuser
CMS_DB_PASSWORD=your-secure-password-here

# Redis (internal, no password needed)
REDIS_HOST=cms-redis
REDIS_PORT=6379

# Slack notifications (optional)
SLACK_WEBHOOK_URL=
